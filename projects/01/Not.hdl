/**
 * Not gate:
 * out = not in
 */

CHIP Not {
    IN in;
    OUT out;

    PARTS:
    Nand (a=in, b=in, out=n1);
    Nand (a=n1, b=in, out=n2);
    Nand (a=n2, b=in, out=out);
}
